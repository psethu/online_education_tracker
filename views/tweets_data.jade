extends layout

block content
	h1= "Online education trends"
	br
	table(class="tweets_data")
		thead
			tr
				th Date
				th Tweet
		tbody
		each val in data
			tr
				td=val.date
				td(class="tweet_class")=val.tweet
	br
	br
	
	
	form(name="date_filter", class="form_border")
		h3 Filter by date
		| From:  &nbsp;
		input(class="datepicker")
		br
		br
		| To: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		input(class="datepicker")
		p
		input(type="submit")